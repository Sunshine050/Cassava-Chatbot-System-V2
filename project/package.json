{
  "name": "cassava-chatbot-system",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "backend": "cd backend && nodemon server.js",
    "backend:prod": "cd backend && node server.js",
    "setup": "npm install && cd backend && npm install && cd ..",
    "start:all": "concurrently \"npm run backend\" \"npm run dev\"",
    "start:production": "concurrently \"npm run backend:prod\" \"npm run preview\"",
    "test:api": "curl http://localhost:3001/health",
    "postinstall": "cd backend && npm install",
    "deploy:vercel": "npm run build:production && vercel --prod",
    "deploy:render": "npm run build:production"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lucide-react": "^0.344.0",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.8.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.19"
  }
}
